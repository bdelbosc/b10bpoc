<?xml version="1.0"?>
<component name="b10b-convert-config">
  <extension target="org.nuxeo.ecm.core.convert.service.ConversionServiceImpl" point="configuration">
    <configuration>
      <!--24h retention-->
      <gcInterval>1440</gcInterval>
      <!-- 25 GB of disk cache-->
      <diskCacheSize>26214400</diskCacheSize>
      <enableCache>true</enableCache>
    </configuration>
  </extension>
</component>
