<?xml version="1.0"?>
<component name="b10b-binary-metadata-config">
  <require>org.nuxeo.binary.metadata</require>

  <!-- disabling action on aboutToCreate -->
  <extension target="org.nuxeo.ecm.core.event.EventServiceComponent" point="listener">
    <listener name="binaryMetadataSyncListener"
      class="org.nuxeo.binary.metadata.internals.listeners.BinaryMetadataSyncListener"
      priority="100">
      <event>beforeDocumentModification</event>
    </listener>
  </extension>

</component>
